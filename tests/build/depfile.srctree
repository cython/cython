CYTHONIZE -M foo.pyx
PYTHON -c'import check.py'

######## foo.pyx ########

from bar cimport empty

include "baz.pxi"

empty()
print(foo())


######## baz.pxi ########

def foo():
    return "foo"



######## bar.pxd ########

cdef inline void empty():
    print("empty")




######## check.py ########

import os

with open("foo.c.dep", "r") as f:
    contents = f.read().replace("\n", " ").replace("\\", "")

assert sorted(contents.split()) == ['bar.pxd', 'baz.pxi', 'foo.c:', 'foo.pyx']
