assert b1 == 4
cdef cypkg.b.b_type b2 = 4
assert b2 == 4


cimport cypkg.sub
cdef cypkg.sub.a_type a3 = 3
assert a3 == 3
cdef cypkg.sub.a.a_type a4 = 3
